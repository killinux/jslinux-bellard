			      ._M_sequence(*this, "this"));
	this->splice(__position, _GLIBCXX_MOVE(__x), __x.begin(), __x.end());
      }

      void
#ifdef __GXX_EXPERIMENTAL_CXX0X__
      splice(iterator __position, list&& __x, iterator __i)
